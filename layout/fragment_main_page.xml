<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainPageFragment">

    <ImageView
        android:id="@+id/image_ny_style_pizza"
        android:layout_width="94dp"
        android:layout_height="90dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="40dp"
        android:src="@drawable/ny_style_pizza"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <ImageView
        android:id="@+id/image_chicago_style_pizza"
        android:layout_width="121dp"
        android:layout_height="77dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="40dp"
        android:src="@drawable/chicago_style_pizza"
        app:layout_constraintStart_toEndOf="@+id/image_ny_style_pizza"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <ImageView
        android:id="@+id/image_store_orders"
        android:layout_width="98dp"
        android:layout_height="86dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="116dp"
        android:src="@drawable/store_orders"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/image_ny_style_pizza" />

    <ImageView
        android:id="@+id/image_current_order"
        android:layout_width="103dp"
        android:layout_height="100dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="104dp"
        android:src="@drawable/current_order"
        app:layout_constraintStart_toEndOf="@+id/image_store_orders"
        app:layout_constraintTop_toBottomOf="@+id/image_chicago_style_pizza" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="66dp"
        android:text="@string/ny_style"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/image_ny_style_pizza" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:text="@string/chicago_style"
        app:layout_constraintStart_toStartOf="@+id/image_chicago_style_pizza"
        app:layout_constraintTop_toBottomOf="@+id/image_chicago_style_pizza" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="66dp"
        android:text="@string/store_orders"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/image_store_orders" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:text="@string/current_order"
        app:layout_constraintStart_toStartOf="@+id/image_current_order"
        app:layout_constraintTop_toBottomOf="@+id/image_current_order" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="100dp"
        android:layout_height="29dp"
        android:layout_marginStart="148dp"
        android:layout_marginTop="124dp"
        android:text="Scarlet Pizza"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>